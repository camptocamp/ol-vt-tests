{
  "name": "ol-vt-tests",
  "version": "0.0.1",
  "description": "Tests & metrics on vector tiles support in openlayers",
  "scripts": {
    "start": "webpack-dashboard -- webpack-dev-server",
    "dev": "NODE_ENV=dev webpack-dev-server --hot --inline --host 0.0.0.0 --port 3000 -d",
    "build": "webpack --config webpack.config.js && cp -r data/ dist",
    "deploy": "npm run build && gh-pages --dist dist --add"
  },
  "dependencies": {
    "mapbox-gl": "^0.44.1",
    "mapbox-to-ol-style": "^3.4.0",
    "ol-mapbox-style": "^2.10.1",
    "openlayers": "https://api.github.com/repos/openlayers/openlayers/tarball/18fa8dd",
    "worker-loader": "^1.1.1"
  },
  "devDependencies": {
    "css-loader": "^0.28.9",
    "gh-pages": "^1.1.0",
    "html-webpack-plugin": "^2.30.1",
    "ls": "0.2.1",
    "style-loader": "^0.20.1",
    "webpack": "^3.4.1",
    "webpack-dashboard": "^0.4.0",
    "webpack-dev-server": "^2.6.1"
  }
}
